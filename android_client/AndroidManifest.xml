<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.example.parking">
    <uses-sdk android:minSdkVersion="11" android:targetSdkVersion="17" />
    <application android:icon="@drawable/icon" android:label="@string/app_name" android:theme="@android:style/Theme.Holo">
        <activity android:name=".Main">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".ParkingState"/>
    </application>
</manifest>
